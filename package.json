{
  "name": "angular-jest",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "dev:ssr": "ng run angular-jest:serve-ssr",
    "serve:ssr": "node dist/angular-jest/server/main.js",
    "build:ssr": "ng build && ng run angular-jest:server",
    "prerender": "ng run angular-jest:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/platform-server": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@nguniversal/express-engine": "^16.0.2",
    "express": "^4.15.2",
    "git": "^0.1.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.4",
    "@angular/cli": "~16.0.4",
    "@angular/compiler-cli": "^16.0.0",
    "@nguniversal/builders": "^16.0.2",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^14.15.0",
    "jest": "^29.5.0",
    "jest-preset-angular": "^13.1.1",
    "typescript": "~5.0.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "globalSetup": "jest-preset-angular/global-setup"
  }
}